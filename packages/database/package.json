{
  "name": "@repo/database",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "build:tsc": "tsc --project tsconfig.build.json",
    "generate": "doppler run -- drizzle-kit generate",
    "migrate": "doppler run -- drizzle-kit migrate",
    "push": "doppler run -- drizzle-kit push",
    "studio": "doppler run -- drizzle-kit studio",
    "db:seed": "doppler run -- tsx src/seed.ts",
    "doppler-setup": "doppler setup --no-interactive"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.5",
    "@repo/shared": "workspace:*",
    "@types/ws": "^8.18.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "uuidv7": "^1.0.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.5.4",
    "drizzle-kit": "^0.24.2",
    "tsup": "^8.5.0",
    "tsx": "^4.16.5",
    "typescript": "^5.6.2"
  }
}
